{layout ../@layout.latte}

{block title}{$product->getName()}{/block}
{block content}
<h1>Product Detail: {$product->getName()}</h1>

<div class="productDetail">
	<div class="catalog-item">
		<div class="catalog-item-image" >
			<img src="{$product->getImageUrl()|nocheck}" alt="{$product->getName()}">
		</div>
		<div class="catalog-item-details">
			<div class="catalog-item-details-name">
				<a n:href="ProductDetail: id => $product->getId()">
					{$product->getName()}
				</a>
			</div>
			<div class="catalog-item-details-description">{$product->getDescription()}</div>
			<div class="catalog-item-details-price">${$product->getPrice()}</div>
		</div>
		<div class="catalog-item-addToBasket">
			{control addToBasket}
		</div>
	</div>

	<div class="reviews">
		<div class="reviews-overall">
			Reviews: mixed
		</div>

		<div class="reviews-list">
			<div class="review">
				<div class="review-author-portrait">
					<img src="data:image/png;base64," alt="Road Runner">
				</div>
				<div class="review-author-name">
					Road Runner
				</div>
				<div class="review-ranking review-ranking-positive">
					5 out of 5
				</div>
			</div>

			<div class="review">
				<div class="review-author-portrait">
					<img src="http://icons.iconarchive.com/icons/sykonist/looney-tunes/256/Wile-E-Coyote-icon.png" alt="Wile E Coyote">
				</div>
				<div class="review-author-name">
					Wile E Coyote
				</div>
				<div class="review-ranking review-ranking-negative">
					0 out of 5
				</div>
			</div>
		</div>
	</div>
</div>
